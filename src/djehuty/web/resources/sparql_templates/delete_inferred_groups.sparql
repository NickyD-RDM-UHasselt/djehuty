{% extends "prefixes.sparql" %}
{% block query %}
DELETE {
    GRAPH <{{state_graph}}> {
    ?group      ?predicate          ?object .
    }
}
WHERE {
  GRAPH <{{state_graph}}> {
    ?group      rdf:type            djht:InstitutionGroup .
    ?group      djht:is_inferred    "true"^^xsd:boolean .
    ?group      ?predicate          ?object .
    ?account    rdf:type            djht:Account .
  }
}
{% endblock %}
